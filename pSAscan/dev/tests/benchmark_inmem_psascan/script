#!/bin/bash
numactl -i 0-1 ./bench $@
numactl -i 0-1 -C 0-15,18-21 ./bench $@
numactl -i 0-1 -C 0-13,18-19 ./bench $@
numactl -i 0-1 -C 0-11 ./bench $@
numactl -i 0-1 -C 0-3,6-9 ./bench $@
numactl -i 0-1 -C 0-2,6-8 ./bench $@
numactl -i 0-1 -C 0-1,6-7 ./bench $@
numactl -i 0-1 -C 0,6 ./bench $@
numactl -i 0-1 -C 0 ./bench $@
